#include <iostream>
#include <list>
#include <string>
using namespace std;

int main() {
    list<int> numbers;
    numbers.push_back(1);
    numbers.push_back(2);
    numbers.push_back(3);
    numbers.push_front(0); 

    
    list<string> words;
    words.push_back("Hello");
    words.push_back("World");
    words.push_back("!");

    return 0;
}
